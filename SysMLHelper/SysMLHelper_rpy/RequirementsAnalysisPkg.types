
[Types]
Names=Feature Requirements,System Requirements,Usage Requirements

[Feature Requirements]
Tool=DoorsXML
ToolEdit=Doors
Image=doors
Variable1Name=profile
Variable1Label=Profile
Variable1Type=dynenum
Variable1EnumValuesMethod=getProfileNames
Variable2Name=server
Variable2Label=Server
Variable2Type=string
Variable3Name=baseline
Variable3Label=Baseline
Variable3Type=string
IsFileBased=0
Section1Name=Object
Section1Tree=<Object number="$i"><Attribute value="$l" name="Object Heading"></Attribute><Picture>$g</Picture></Object>
Requirement1Name=Requirement
Requirement1Tree=<Object id="$i"><Picture>$g</Picture><Text>$t</Text><Attribute name="Object Type" value="Requirement"></Attribute></Object>
TextFormat=1
Type=xml

